{
  "include": ["vite.config.ts"],
  "compilerOptions": {
    "lib": ["ES2022"],
    "types": ["node"],
    "strict": true,
    "noEmit": true,
    "module": "ESNext",

    "skipLibCheck": true,
    "composite": true,

